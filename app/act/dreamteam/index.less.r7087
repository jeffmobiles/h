/* ��ע */
.foc_contain {
    position: relative;
    .btn_focus {
        position: absolute;
        left: 50%;
        top: 50%;
        width: 1.94rem;
        height: 0.4rem;
        border: 2px solid #80c269;
        border-radius: 5px;
        color: #9edac3;
        text-align: center;
        line-height: 0.4rem;
        transform: translate(-50%,-50%);
        -ms-transform: translate(-50%,-50%);
        /* IE 9 */
        -webkit-transform: translate(-50%,-50%);
        /* Safari and Chrome */
        font-size: 0.22rem;
    }
    .surprised {
        position: absolute;
        left: 50%;
        top: 50%;
        color: #fff;
        line-height: 0.4rem;
        transform: translate(-50%,-50%);
        -ms-transform: translate(-50%,-50%);
        /* IE 9 */
        -webkit-transform: translate(-50%,-50%);
        /* Safari and Chrome */
        font-size: 0.24rem;
    }
}
.qiu {
    position: absolute;
    top: 7.7rem;
    width: 0.80rem;
    height: 0.87rem;
    transform: translate(-50%,0);
    -ms-transform: translate(-50%, 0);
    -webkit-transform: translate(-50%, 0);
    left: 50%;
}
.dot {
    width: 0.95rem;
    height: 0.92rem;
    border-radius: 100%;
    position: absolute;
    z-index: 0;
    animation: sploosh 2s infinite cubic-bezier(0.165, 0.84, 0.44, 1);
    -webkit-animation: sploosh 2s infinite cubic-bezier(0.165, 0.84, 0.44, 1);
    -moz-animation: 2s infinite cubic-bezier(0.165, 0.84, 0.44, 1);
    -moz-animation-name: sploosh;
    background: transparent;
}
.xu {
    width: 0.95rem;
    height: 0.92rem;
    animation: xuan 2s 0.5s infinite linear;
    -webkit-animation: xuan 2s 0.5s infinite linear;
    -moz-animation: xuan 0.5s 0.5s infinite linear;
    -ms-animation: xuan 0.5s 0.5s infinite linear;
    -o-animation: xuan 0.5s 0.5s infinite linear;
    -webkit-animation-play-state: running;
    -moz-animation-play-state: running;
    -ms-animation-play-state: running;
    -o-animation-play-state: running;
    animation-play-state: running;
}
@-webkit-keyframes xuan {
    0% {
        -webkit-transform: rotateZ(0deg);
    }
    50% {
        -webkit-transform: rotateZ(180deg);
    }
    100% {
        -webkit-transform: rotateZ(360deg);
    }
}
@-ms-keyframes xuan {
    0% {
        -webkit-transform: rotateZ(0deg);
    }
    50% {
        -webkit-transform: rotateZ(180deg);
    }
    100% {
        -webkit-transform: rotateZ(360deg);
    }
}
@keyframes xuan {
    0% {
        -webkit-transform: rotateZ(0deg);
    }
    50% {
        -webkit-transform: rotateZ(180deg);
    }
    100% {
        -webkit-transform: rotateZ(360deg);
    }
}
@-webkit-keyframes sploosh {
    0% {
        box-shadow: 0 0 0 0 rgba(66, 166, 223, 0.7);
        background: rgba(66, 166, 223, 0.7);
    }
    100% {
        box-shadow: 0 0 0 300px rgba(66, 166, 223, 0);
        background: rgba(66, 166, 223, 0);
    }
}
@-moz-keyframes sploosh {
    0% {
        box-shadow: 0 0 0 0 rgba(66, 166, 223, 0.7);
        background: rgba(66, 166, 223, 0.7);
    }
    100% {
        box-shadow: 0 0 0 300px rgba(66, 166, 223, 0);
        background: rgba(66, 166, 223, 0);
    }
}
@-ms-keyframes sploosh {
    0% {
        box-shadow: 0 0 0 0 rgba(66, 166, 223, 0.7);
        background: rgba(66, 166, 223, 0.7);
    }
    100% {
        box-shadow: 0 0 0 300px rgba(66, 166, 223, 0);
        background: rgba(66, 166, 223, 0);
    }
}
@-o-keyframes sploosh {
    0% {
        box-shadow: 0 0 0 0 rgba(66, 166, 223, 0.7);
        background: rgba(66, 166, 223, 0.7);
    }
    100% {
        box-shadow: 0 0 0 300px rgba(66, 166, 223, 0);
        background: rgba(66, 166, 223, 0);
    }
}
@keyframes sploosh {
    0% {
        box-shadow: 0 0 0 0 rgba(66, 166, 223, 0.7);
        background: rgba(66, 166, 223, 0.7);
    }
    100% {
